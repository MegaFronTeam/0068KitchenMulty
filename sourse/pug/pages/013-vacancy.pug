block pagecontent
	+mob-mnu()
	+header()
		+topLine()
		+top-nav()
		+brands()
	+b.page-head
		.container
			+breadcrumb('Главная', 'Вакансии')
			h1 Вакансии
	+sVacancy()
	+sFormAlt({
		title: 'Заинтересовала вакансия?',
		descr: 'Свяжись с нами, заполнив форму. Мы перезвоним тебе и все обсудим!',
	})
	+footer()