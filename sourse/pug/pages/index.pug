include ../blocks/sGForm/_sGForm.pug
include ../blocks/sBrends/_sBrends.pug
include ../blocks/sTags/_sTags.pug

block variables
	- let  mainPage= null 
	- let  bg= 'index.png'

block title
	- let  title= 'title'
	title= title
block pagecontent
	+mob-mnu()
	+header()
		+topLine()
		+top-nav()
		//- +brands()
	+headerBlock()
	+sFamiliar({
		title: `Последние поступления`,
		bgAlt: true,
		addClasses: 'dark-bg absolute-title',
		absoluteTitle: true,
	})
	+sPopular()
	+sNew()
	+sIconsAlt()
	+sBest()
	+sGForm()
	+sBrends()
	+sTags()
	+footer()